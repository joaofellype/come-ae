    <section class="tarja-endereco hide-on-med-and-down">
        <div class="container">
            <div class="endereco-cadastrado right">
                <a href="#modalEntrada" class="cor-primaria entrega-dados modal-trigger"><i
                        class="icon-regiao cor-secundaria"></i>
                    <span class="cor-secundaria entrega">Entregar em:</span>
                    <span class="ende"></span>
                </a>
            </div>
        </div>
    </section>
    <section class="tarja-endereco-mobile hide-on-large-only">
        <div class="container">
            <div class="endereco-cadastrado-mobile">
                <a href="#modalEntrada" class="cor-primaria entrega-dados-mobile modal-trigger"><i
                        class="icon-regiao cor-secundaria"></i>
                    <span class="cor-secundaria entrega">Entregar em:</span>
                    <span class="ende"></span>
                </a>
            </div>
        </div>
    </section>

    <section class="sec-list-help">

        <div class="header-busca">
            <span class="cor-primaria">Como podemos ajudar você?</span>
            <div class="input-field">
                <i class="material-icons">search</i>
                <i class="material-icons cancelarbusca" style="display: none;">close</i>
                <input class="white-text buscarajuda" placeholder="Digite aqui a sua busca..." id="buscarajuda"
                    type="text">
            </div>
        </div>

        <div class="row lista-ajuda">
            <div class="contato-comeae">
                <span class="purple-text kit-flex back-listhelp">Cancelar</span>
                <span class="kit-flex grey-text">É preciso entrar na sua conta para ter um atendimento
                    personalizado</span>
                <a href="/ent_cad" class="red-text kit-flex">Entrar</a>
                <div id="form-help">
                    <div class="input-field s12 select-motivo">
                        <select id="motivo-ajuda">
                            <option value="sem-motivo">Meu motivo não está aqui</option>
                            <option value="pedido">Pedido</option>
                            <option value="Pagamento">Pagamento</option>
                            <option value="Conta e Privacidade">Conta e Privacidade</option>
                        </select>
                        <label>Motivo da Ajuda *</label>
                    </div>
                    <div class="input-field s12 select-detalhe">
                        <select id="detalhe-motivo"></select>
                        <label>Detalhe do Motivo *</label>
                    </div>
                    <div class="input-field s12">
                        <input oninput="this.value = this.value.replace(/[^0-9]/g, '').replace(/(\..*)\./g, '$1');"
                            id="pedido" inputmode="numeric" type="text">
                        <label for="pedido">Pedido</label>
                    </div>
                    <div class="input-field s12">
                        <input required id="nomeajuda" type="text" class="validate">
                        <label for="nomeajuda">Nome *</label>
                    </div>
                    <div class="input-field s12">
                        <input oninput="validate()" onfocus="validate()" required id="email" type="email">
                        <label for="email">Email *</label>
                    </div>
                    <div class="input-field s12">
                        <input required id="telefoneajuda" maxlength="15" minlength="14" type="tel" inputmode="numeric">
                        <label for="telefoneajuda">Telefone *</label>
                    </div>
                    <div class="input-field s12">
                        <input required id="oqaconteceu" type="text" class="validate">
                        <label for="oqaconteceu">O que aconteceu? *</label>
                    </div>
                    <button class="waves-effect waves-light btn-small bt-enviar-ajuda purple darken-3"
                        disabled>Enviar</button>
                </div>
