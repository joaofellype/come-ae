                    <span class="cor-secundaria entrega">Entregar em:</span>
                    <span class="ende"></span>
                </a>
            </div>
        </div>
    </section>

    <section class="cupons container">
        <div>
            <span class="fonte-primaria kit-flex center cor-primaria">Cupons</span>
            <div class="linha-titulo linha-titulo-primaria"></div>
        </div>
        <div class="row">
            <div class="col s12 dv-tabs-cups">
                <ul class="tabs tabs-cupons">
                    <li class="tab col s6 center"><a href="#tb-ativos">Ativos (0)</a></li>
                    <li class="tab col s6 center"><a href="#tb-inativos">Inativos (0)</a></li>
                </ul>
            </div>
            <div id="tb-ativos" class="col s12">
                <div class="row dv-cp-ativos">
                    <div class="card col s6 card-cupom">
                        <div class="card-content">
                            <div class="kit-flex">
                                <i class="material-icons">confirmation_number</i>
                                <span class="card-title grey-text text-darken-4 kit-flex">Cupom de R$<b
                                        class="cup-val">10</b></span>
                            </div>
                            <div class="dv-info-cup">
                                <span class="cup-info">Garanta antes que
                                    acabe! Só com pagamento pelo ComeÂe.</span>
                            </div>
                            <div class="kit-flex">
                                <span class="kit-flex activator">Ver regras<i
                                        class="material-icons">arrow_drop_down</i></span>
                                <span class="cup-expirado">Expirado em 11/02</span>
                            </div>
                            <div class="kit-flex">
                                <i class="material-icons">access_time</i>
                                <span class="cup-data">O prazo de validade terminou</span>
                            </div>
                        </div>
                        <div class="card-reveal dv-reveal-cup">
                            <div class="kit-flex">
                                <i class="material-icons">confirmation_number</i>
                                <span class="card-title grey-text text-darken-4 kit-flex">Cupom de R$<b
                                        class="cup-val">10</b></span>
                            </div>
                            <div class="dv-regras">
                                <span>Garanta antes que acabe! Só com
                                    pagamento pelo ComeAê.</span>
                                <ul>
